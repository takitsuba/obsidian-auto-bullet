		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.25.0",
		"jest": "^29.7.0",
		"jest-environment-obsidian": "^0.0.1",
		"jest-message-util": "^29.7.0",
		"obsidian": "latest",
		"ts-jest": "^29.2.6",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
